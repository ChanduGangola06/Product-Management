{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://product-management-server-xi.vercel.app/api/$1" },
    { "source": "/health", "destination": "https://product-management-server-xi.vercel.app/health" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}


