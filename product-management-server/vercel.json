{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "functions": {
    "src/index.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/health",
      "dest": "/src/index.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/index.ts"
    }
  ]
}
